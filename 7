package main

import "fmt"

func main() {
	slice := make([]int, 0, 5)

	var numero int
	fmt.Print("Informe um n√∫mero inteiro: ")
	fmt.Scan(&numero)

	jaPresente := false
	for _, v := range slice {
		if v == numero {
			jaPresente = true
			break
		}
	}

	if !jaPresente {
		slice = append(slice, numero)
	}

	fmt.Println("Slice resultante:", slice)
}

package main

import "fmt"

func main() {
	array := [10]float64{2.5, 6.0, 8.3, 3.2, 9.7, 4.1, 7.8, 1.2, 5.6, 10.0}

	var maioresQue5 []float64

	for _, valor := range array {
		if valor > 5 {
			maioresQue5 = append(maioresQue5, valor)
		}
	}

	fmt.Println("Novo Slice com elementos maiores que 5:", maioresQue5)
}
